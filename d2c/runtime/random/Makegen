if ($host_platform{'integer_length'} >= 64) {
        $CFLAGS = "$CFLAGS -DDYLAN_64BIT";
}
$CPPFLAGS = "$CPPFLAGS -I.. ";
	
$D2CFLAGS         # added by update-libdirs
    = $d2c_runtime
    . ' -L../transcendental';

do emit_library_rule(
    'Random', '$(BUILDROOT)/force.timestamp', '', 'compile', 'install'
);
